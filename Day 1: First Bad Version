https://leetcode.com/explore/featured/card/may-leetcoding-challenge/534/week-1-may-1st-may-7th/3316/


class Solution {
public:
    int firstBadVersion(int n) {
        
        long long int low = 1;
        long long int high = n;
       
        while(low<high){
            int mid = (low+high)/2;
            
            if(isBadVersion(mid)==true){
                high = mid;
            }
            else
                low = mid + 1;
        }
        
        return low;
    }
};
